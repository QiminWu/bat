[clusterparameter]
#clustering parameter
beginMinStationDistance = 1
endMinStationDistance=59.9
beginMaxStationDistance=60
endMaxStationDistance=90
maxCluster = 10
minClusterStation = 3
cutoff = 2
#unit in deegree
centroidminDistance = 10
comparedelta = 10
#unit in km
clusterdistance = 10
stationdistance = 50
#algorithm method
#1 zoning
#2 kmeans
cam = 2

[general parameter]
dimx = 40
dimy = 40
minDist = 30
maxDist = 90
step = 5
winlen = 30
forerun = 200
duration = 500
security = 60
gridspacing = 0.2
ttphase=P
export_unfiltered=false
export_filtered=false
export_resampled=false
new_frequence = 10

[skript parameter]

xcorr=1

plotcopy=plot1,plot2,plot3,plot4,rerun
plot1 = semblance-plot.sh
plot2 = summary-plot.sh
plot3 = plot-cluster.sh
rerun= rerun_sum.py
plot4 = plot-arraycluster.sh
