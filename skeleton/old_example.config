[beamforming method]
#delaysum
#capon
beam = delaysum

[filterparameter]
filterswitch=2
###############################################################
#Parameter for filtering 
#bp butterworth

# low cut corner frequency
flo = 0.4

# high cut corner frequency
fhi = 3

# number of filter sections
ns = 3

# TRUE -> zero phase filter
zph = false

###############################################################
#Parameter for filtering 
#lowpass butterworth
l_fc = 1.5
# number of filter sections
l_ns = 4
# TRUE -> zero phase filter
l_zph = false

###############################################################
#Parameter for filtering 
#highpass butterworth
h_fc = 2
# number of filter sections
h_ns = 4
# TRUE -> zero phase filter
h_zph = false

